<?xml version="1.0" encoding="UTF-8"?>
<testcase expect="invalid" xmlns="https://doi.org/10.5281/zenodo.5679629#">
  <testcase:label xmlns:testcase="https://doi.org/10.5281/zenodo.5679629#">The svrl:successful-report/@location attribute contains the location of the successful report</testcase:label>
  <schemas>
    <sch:schema xmlns:sch="http://purl.oclc.org/dsdl/schematron">
      <sch:pattern>
        <sch:rule context="*">
          <sch:report test="true()"/>
        </sch:rule>
      </sch:pattern>
    </sch:schema>
  </schemas>
  <documents>
    <document filename="document.xml">
      <document xmlns=""/>
    </document>
  </documents>
  <assertions xmlns:svrl="http://purl.oclc.org/dsdl/svrl">
    <assertion assert="contains(//svrl:successful-report/@location, 'document')"/>
  </assertions>
</testcase>